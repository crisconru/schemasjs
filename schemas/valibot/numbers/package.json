{
  "name": "@schemasjs/valibot-numbers",
  "version": "1.0.13",
  "description": "Javascript (and Typescript) valibot number schemas",
  "author": "Cristobal Contreras Rubio",
  "license": "MIT",
  "homepage": "https://github.com/crisconru/schemasjs/tree/master/schemas/valibot/numbers/readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crisconru/schemasjs.git"
  },
  "bugs": {
    "url": "https://github.com/crisconru/schemasjs/issues"
  },
  "keywords": [
    "uint",
    "int",
    "float",
    "c-types",
    "valibot",
    "types",
    "schemas",
    "modular",
    "typescript",
    "schema",
    "validation",
    "parsing",
    "type-safe",
    "runtime"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "lint": "ts-standard",
    "format": "ts-standard --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "valibot": ">0.31.0"
  },
  "ts-standard": {
    "ignore": [
      "tests",
      "vitest.config.ts",
      "tsup.config.ts"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  }
}
