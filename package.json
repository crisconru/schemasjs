{
  "name": "schemasjs",
  "version": "1.0.0",
  "private": true,
  "author": "Crist√≥bal Contreras Rubio",
  "description": "Collection of schemas for validators like valibot, zod, etc",
  "license": "MIT",
  "homepage": "https://github.com/crisconru/schemasjs#readme",
  "bugs": {
    "url": "https://github.com/crisconru/schemasjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crisconru/schemasjs.git"
  },
  "keywords": [
    "schemas",
    "typescript",
    "zod",
    "valibot",
    "type-validation",
    "validator"
  ],
  "type": "module",
  "main": "index.js",
  "workspaces": [
    "schemas/valibot/*",
    "schemas/zod/*"
  ],
  "scripts": {
    "build:valibot-numbers": "npm run build --workspace=@schemasjs/valibot-numbers",
    "test:valibot-numbers": "npm run test --workspace=@schemasjs/valibot-numbers",
    "coverage:valibot-numbers": "npm run coverage --workspace=@schemasjs/valibot-numbers",
    "build:zod-numbers": "npm run build --workspace=@schemasjs/zod-numbers",
    "test:zod-numbers": "npm run test --workspace=@schemasjs/zod-numbers",
    "coverage:zod-numbers": "npm run coverage --workspace=@schemasjs/zod-numbers"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.3.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
